  {
    "containerDefinitions": [
      {
        "name": "cherubgyre-prod",
        "image": ":latest",
        "cpu": 0,
        "portMappings": [
          {
            "name": "rust-api-container-8080-tcp",
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "appProtocol": "http"
          }
        ],
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/cherubgyre-dev",
            "awslogs-region": "us-east-1",
            "awslogs-create-group": "true",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "family": "cherubgyre-dev",
    "executionRoleArn": "arn:aws:iam::545009831939:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
    }
  }
